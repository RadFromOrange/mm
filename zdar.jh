


/**
 * The Employee entity.
 */
entity Employee {
	clientId String,
	advisorId String,
	appointmentId String,
	roomId String,
    token String,
	endDate Instant,
    startDate Instant,

}


enum Language {
    FRENCH, ENGLISH, SPANISH
}





// Use Data Transfert Objects (DTO)
dto * with mapstruct

// Set service options to all except few
service all with serviceImpl 

